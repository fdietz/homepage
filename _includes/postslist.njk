<ul reversed class="postlist list-unstyled">
{% for post in postslist | reverse %}
  <li class="postlist-item">
    <a href="{{ post.url | url }}" class="postlist-link">{% if post.data.title %}{{ post.data.title }}{% endif %}</a>
    <div class="postlist-meta">
      <time datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate }}</time>
      <span class="divider">â€¢</span>
      <span>{{ post.templateContent | readingTime }} read</span>
    </div>
  </li>
{% endfor %}
</ul>
